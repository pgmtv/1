def signal_handler(signum, frame):
    global stop_recording
    print("Interrupção detectada. Passando para os próximos canais...")
    stop_recording = True

signal.signal(signal.SIGINT, signal_handler)

import streamlink

def download_and_save_stream(url, channel_name, duration=240, user_agent=None):
    global stop_recording
    if stop_recording:
        return

    timezone_sp = pytz.timezone('America/Sao_Paulo')
    now_utc = datetime.datetime.utcnow()
    now_sp = timezone_sp.fromutc(now_utc)
    now = now_sp.strftime("%m%d_%H%M%S")
    file_name = f"{now}_SBTVD_{channel_name}_{datetime.datetime.now().year}.ts"
    output_path = os.path.join(output_folder, file_name)

    if user_agent:
        streamlink.set_option("http-headers", f"User-Agent={user_agent}")

    try:
        streams = streamlink.streams(url)
        if "best" in streams:
            with streams["best"].open() as stream_fd:
                with open(output_path, "wb") as output_fd:
                    print(f" Gravando {channel_name} do link: {url}")
                    start_time = datetime.datetime.now()
                    while datetime.datetime.now() < start_time + datetime.timedelta(minutes=duration):
                        if stop_recording:
                            break
                        output_fd.write(stream_fd.read(1024 * 1024))  # 1MB buffer
    except Exception as e:
        print(f"Interrupção na gravação do {channel_name}: {e}")


