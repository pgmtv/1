import subprocess
import pytz
import concurrent.futures
import threading
import streamlink

drive.mount('/content/drive')

output_folder = '/content/drive/MyDrive/TEVE/'

stop_recording = False
num_channels_recorded = 0
lock = threading.Lock()

def signal_handler(signum, frame):
    global stop_recording
    print("Interrupção detectada. Passando para os próximos canais...")
    stop_recording = True

signal.signal(signal.SIGINT, signal_handler)

def download_and_save_ffmpeg(url, channel_name, duration=7, user_agent=None):
    global stop_recording, num_channels_recorded

    if stop_recording:
        return

    timezone_sp = pytz.timezone('America/Sao_Paulo')
    now_utc = datetime.datetime.utcnow()
    now_sp = timezone_sp.fromutc(now_utc)
    now = now_sp.strftime("%m%d_%H%M%S")
    file_name = f"{now}_SBTVD_{channel_name}_{datetime.datetime.now().year}.mp4"
    output_path = os.path.join(output_folder, file_name)

    command = ["ffmpeg", "-i", url, "-c:v", "copy", "-t", str(duration * 60), output_path, "-reconnect_streamed", "1"]

    if user_agent:
        command.insert(1, "-user_agent")
        command.insert(2, user_agent)

    print(f"Gravando {channel_name} do link: {url}")

    try:
        process = subprocess.Popen(command, stdout=subprocess.PIPE, stderr=subprocess.PIPE)
        _, _ = process.communicate(timeout=duration * 60)
        with lock:
            num_channels_recorded += 1
    except subprocess.TimeoutExpired:
        print(f"Tempo limite atingido para a gravação de {channel_name}. Passando para o próximo canal...")
        process.kill()  # Encerra o processo se atingir o tempo limite
    except subprocess.CalledProcessError as e:
        print(f"Falha ao gravar {channel_name}: {e}")
    except Exception as e:
        print(f"Interrupção na gravação do {channel_name}: {e}")

def download_and_save_streamlink(url, channel_name):
    global stop_recording, num_channels_recorded

    if stop_recording:
        return

    timezone_sp = pytz.timezone('America/Sao_Paulo')
    now_utc = datetime.datetime.utcnow()
    now_sp = timezone_sp.fromutc(now_utc)
    now = now_sp.strftime("%m%d_%H%M%S")
    file_name = f"{now}_SBTVD_{channel_name}_{datetime.datetime.now().year}.ts"
    output_path = os.path.join(output_folder, file_name)

    print(f"Gravando {channel_name} do link: {url}")

    try:
        duration = "00:04:00"  # Set the desired duration here (e.g., 4 minutes)
        command = ["streamlink", "--twitch-disable-ads",url, "best", "-o", output_path, "--hls-duration", duration]
        subprocess.run(command, check=True)

        with lock:
            num_channels_recorded += 1
    except subprocess.CalledProcessError as e:
        print(f"Falha ao gravar {channel_name}: {e}")
    except Exception as e:
        print(f"Interrupção na gravação do {channel_name}: {e}")


